<div class="col-2" style="width: 20.1%;position: fixed;">
      <nav class="col-md-2 d-none d-md-block bg-white sidebar" style="height:100vh; width: auto;">
          <div class="sidebar-sticky">
          <br>
            <ul class="nav flex-column" id="navbar">
              <li class="nav-item my-5">
                <%= link_to root_path, class:"nav-link active text-decoration_none", 'aria-hidden' => true do  %>
                  <div class="d-flex justify-content-start align-items-center mx-3 sidebarlink "><i class="fas fa-home mr-2 mx-3 " style="font-size: 22px;"></i><p class="m-0">Dashboard</p></div>
                <% end %>
              </li>
              <li class="nav-item mb-5">
                <%= link_to tickets_path, class:"nav-link text-decoration_none", 'aria-hidden' => true do %>
                  <div class="d-flex justify-content-start align-items-center mx-3 sidebarlink"><i class="fas fa-ticket mx-3 " style="font-size: 22px;"></i><p class="m-0">Tickets</p></div>
                <% end %>
              </li>
              <li class="nav-item mb-5">
                <%= link_to  mytickets_path, class:"nav-link text-decoration_none", 'aria-hidden' => true do %>
                  <div class="d-flex justify-content-start align-items-center mx-3 sidebarlink"><i class="fas fa-ticket-alt mx-3" style="font-size: 22px;"></i><p class="m-0">My Tickets</p></div>
                <% end %>
              </li>
              <li class="nav-item mb-5">
                <%= link_to chatroom_path(Chatroom.ids), class:"nav-link text-decoration_none", 'aria-hidden' => true do %>
                  <div class="d-flex justify-content-start align-items-center mx-3 sidebarlink"><i class="fas fa-comment mx-3 " style="font-size: 22px;"></i><p class="m-0">Chat</p></div>
                <% end %>
              </li>
              <li class="nav-item mb-5">
                <%= link_to "#", class:"nav-link text-decoration_none", 'aria-hidden' => true do %>
                  <div class="d-flex justify-content-start align-items-center mx-3 sidebarlink"><i class="fas fa-cog mx-3 " style="font-size: 22px;"></i><p class="m-0">Settings</p></div>
                <% end %>
              </li>
            </ul>
          </div>
      </nav>
</div>
